msgid "B<dumpe2fs>(8), B<tune2fs>(8), B<e2fsck>(8), B<mke2fs>(8)"
msgstr "B<dumpe2fs>(8), B<tune2fs>(8), B<e2fsck>(8), B<mke2fs>(8)"

